{
  "name": "ride-finder",
  "version": "1.0.0",
  "description": "Ride finder based on GBFS data",
  "main": "index.js",
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "rm -rf ./dist && tsc -b",
    "start": "bun run build && bun run dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:watch": "bun test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpercitak/ride-finder.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpercitak/ride-finder/issues"
  },
  "homepage": "https://github.com/alpercitak/ride-finder#readme",
  "dependencies": {
    "axios": "^1.1.3",
    "express": "^5.0.0",
    "express-prom-bundle": "^8.0.0",
    "geolib": "^3.3.3",
    "node-cache": "^5.1.2",
    "node-kmeans": "^1.1.9",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.0",
    "@types/bun": "^1.3.6",
    "@types/express": "^5.0.0",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.0.0",
    "supertest": "^7.0.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.6"
}
